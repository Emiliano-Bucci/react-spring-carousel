{
  "name": "react-spring-carousel",
  "version": "3.0.0-beta088",
  "description": "A new <Carousel /> experience for the web",
  "homepage": "https://react-spring-carousel.emilianobucci.com",
  "repository": "https://github.com/Emiliano-Bucci/react-spring-carousel",
  "author": "Emiliano Bucci",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release:beta": "git push && npm run build && npm publish --tag beta"
  },
  "keywords": [
    "react",
    "react-spring",
    "carousel",
    "react-carousel",
    "react-spring-carousel",
    "slider",
    "react-slider",
    "react-spring-slider",
    "animated"
  ],
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/manager-api": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.11",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "@react-spring/web": "^9.7.2",
    "@use-gesture/react": "^10.2.26",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.2.0"
  }
}
